{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@aaa/common-dto/(.*)": "<rootDir>/../libs/common-dto/src/$1",
    "@aaa/common-dto": "<rootDir>/../libs/common-dto/src"
  }
}